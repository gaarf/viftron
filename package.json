{
  "name": "viftron",
  "version": "0.0.1",
  "description": "an electron-based onvif client",
  "license": "UNLICENSED",
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "prebuild": "npm install && ./node_modules/bower/bin/bower update --config.interactive=false",
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "test": "node ./node_modules/karma/bin/karma start test/karma-conf.js",
    "test-single-run": "node ./node_modules/karma/bin/karma start test/karma-conf.js --no-auto-watch --single-run"
  },
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gaarf/viftron"
  },
  "devDependencies": {
    "bower": "^1.3.10",
    "del": "^2.0.2",
    "electron-prebuilt": "^0.34.1",
    "gulp": "^3.8.9",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.3.4",
    "gulp-if": "^2.0.0",
    "gulp-jshint": "^1.8.4",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.2.0",
    "gulp-load-plugins": "^1.0.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-replace": "^0.5.0",
    "gulp-rev": "^6.0.1",
    "gulp-size": "^2.0.0",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.0",
    "gulp-vulcanize": "^6.0.1",
    "gulp-wrapper": "^1.0.0",
    "gulp-zip": "^3.0.2",
    "jasmine-core": "^2.1.2",
    "karma": "^0.13.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.0",
    "merge-stream": "^1.0.0",
    "mkdirp": "^0.5.1"
  },
  "dependencies": {
    "body-parser": "^1.9.3",
    "colors": "^1.0.3",
    "compression": "^1.2.2",
    "envar": "^1.0.0",
    "express": "^4.9.7",
    "finalhandler": "^0.4.0",
    "morgan": "^1.3.1",
    "portfinder": "^0.4.0",
    "q": "^1.4.1",
    "serve-favicon": "^2.1.5"
  }
}
